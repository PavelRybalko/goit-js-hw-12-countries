{{#each this}}
<div class="wrapper">
  <h2 class="country__title"> {{name}}</h2>
  <p class="country__item">Capital: <span class="country__span">{{capital}}</span></p>
  <p class="country__item">Population: <span class="country__span">{{population}}</span></p>
  <p class="country__item">Languages: </p>
  <ul class="languages-list">
    {{#each languages}}
    <li class="languages-list__item">{{this.name}}</li>
    {{/each }}
  </ul>
</div>
{{!-- <div class="country__flag">  --}}
<img src="{{flag}}" alt="flag" height='250' class="country__flag-img">
{{!-- </div> --}}
{{/each }}